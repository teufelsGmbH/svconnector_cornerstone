	"require": {
		"ext-json": "*",
		"typo3/cms-core": "^11.5 || ^12.4",
		"php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
		"cobweb/svconnector": "^5.0 || ^5.1"
