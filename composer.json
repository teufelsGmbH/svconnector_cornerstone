{
	"name": "teufels/svconnector_cornerstone",
	"type": "typo3-cms-extension",
	"description": "Connector service for Cornerstone Reporting API JSON",
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "teufels GmbH",
			"role": "Developer",
			"homepage": "https://teufels.com/"
		},
		{
			"name": "Francois Suter",
			"role": "Developer",
			"homepage": "https://www.ideative.ch/en/"
		}
	],
	"autoload": {
		"psr-4": {
			"Cobweb\\SvconnectorCornerstone\\": "Classes/"
		}
